// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
 int r;int c;
 cout<<"Enter the number of rows: ";
 cin>>r;
 cout<<"Enter the number of column: ";
 cin>>c;
 int m1[100][100];int m2[100][100]; int mul[100][100];
 cout<<"Enter the element of first matrix : ";
 for( int i=0;i<r;i++)
 {
     for(int j=0;j<c;j++)
     {
         cout<<" element at ("<<i+1<< "," <<j+1<< "):";
         cin>>m1[i][j];
     }
 }
  cout<<"Enter the element of second  matrix : ";
 for( int i=0;i<r;i++)
 {
     for(int j=0;j<c;j++)
     {
         cout<<" element at ("<<i+1<< "," <<j+1<< "):";
         cin>>m2[i][j];
     }
 }
  for( int i=0;i<r;i++)
 {
     for(int j=0;j<c;j++)
     {
      mul[i][j]=m1[i][j]*m2[i][j];
     }
 }
 cout<<"multiplicatin of   matrix  is : ";
 for( int i=0;i<r;i++)
 {
     for(int j=0;j<c;j++)
     {
      cout<<mul[i][j]<< " ";
     }
     cout<<endl;
 }
 
 
 
 return 0;
}

