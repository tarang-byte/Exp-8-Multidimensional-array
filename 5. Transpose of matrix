// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int r; int c;
cout<<"Enter the value of row : ";
cin>>r;
cout<<"Enter the value of column :";
cin>>c;
int ar[100][100]; int i;int j; int tr[100][100];
cout<<"Enter the element matrix : ";
for(i=0;i<r;i++){
    for(int j=0;j<c;j++)
    {
        cout<<" element at( "<<i+1<<","<<j+1<< ") : ";
        cin>>ar[i][j];
    }
}
for(i=0;i<r;i++){
    for(int j=0;j<c;j++)
    { 
        tr[i][j]=ar[j][i];
    }
} 
cout<<" the given matrix is ";
for(i=0;i<r;i++){
    for(int j=0;j<c;j++)
    {
        cout<<ar[i][j];
        
    }cout<<endl;
} 
 cout<<" transpose of matrix is : ";
for(i=0;i<r;i++){
    for(int j=0;j<c;j++)
    {
        cout<<tr[i] [j] ;
    }cout<<endl;
}




    return 0;
}

Enter the value of row : 3
Enter the value of column :3
Enter the element matrix :  element at( 1,1) : 3
 element at( 1,2) : 4
 element at( 1,3) : 5
 element at( 2,1) : 6
 element at( 2,2) : 7
 element at( 2,3) : 8
 element at( 3,1) : 9
 element at( 3,2) : 10
 element at( 3,3) : 11
